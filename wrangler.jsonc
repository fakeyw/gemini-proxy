// wrangler.jsonc
{
	// --- 基本信息 ---
	"name": "gemini-proxy",
	"main": "src/worker.ts",
	"compatibility_date": "2025-04-11",
	"durable_objects": {
		"bindings": [
			{
				"name": "API_KEY_MANAGER",
				"class_name": "ApiKeyManager"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_classes": [
				"ApiKeyManager"
			]
		},
		{
			"tag": "v2",
			"new_sqlite_classes": [
				"ApiKeyManager"
			]
		}
	],
	"vars": {
		"UPSTREAM_API_URL": "https://generativelanguage.googleapis.com/v1beta/openai"
	},
	"assets": {
		"directory": "./html",
		"binding": "ASSETS"
	},
	"triggers": {
		"crons": [
			"0 7 * * *"
		]
	}
}