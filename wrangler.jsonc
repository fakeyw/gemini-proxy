// wrangler.jsonc
{
	// --- 基本信息 ---
	"name": "gemini-proxy",
	"main": "src/worker.ts",
	"compatibility_date": "2025-04-11",
	"durable_objects": {
		"bindings": [
			{
				"name": "API_KEY_MANAGER",
				"class_name": "ApiKeyManager"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["ApiKeyManager"]
		}
	],
	"vars": {
		"UPSTREAM_API_URL": "https://generativelanguage.googleapis.com/v1beta/openai"
	},
	"triggers": {
		"crons": [
			"0 7 * * *"
		]
	}
	// --- 构建配置 (可选) ---
	// 如果你的项目结构简单 (如本例)，Wrangler 通常能自动处理构建。
	// 如果有复杂的构建步骤 (例如使用 Webpack/esbuild 的自定义配置)，可以在这里指定。
	// "build": {
	//   "command": "npm run build", // 例如: 如果你需要运行特定的构建脚本
	//   "watch_dir": "src"         // 指定监视哪个目录以触发重新构建
	// }
}